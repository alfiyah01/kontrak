{
  "name": "tradestation-premium",
  "version": "3.0.0",
  "description": "TradeStation Premium Digital Contract System with Advanced Features",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js",
    "dev:debug": "nodemon --inspect server.js",
    "test": "jest --coverage",
    "test:watch": "jest --watch",
    "lint": "eslint . --ext .js",
    "lint:fix": "eslint . --ext .js --fix",
    "format": "prettier --write .",
    "build": "npm run lint && npm run test",
    "backup": "node scripts/backup.js",
    "migrate": "node scripts/migrate.js",
    "seed": "node scripts/seed.js",
    "logs": "tail -f logs/app.log",
    "pm2:start": "pm2 start ecosystem.config.js",
    "pm2:stop": "pm2 stop tradestation-premium",
    "pm2:restart": "pm2 restart tradestation-premium",
    "pm2:logs": "pm2 logs tradestation-premium",
    "docker:build": "docker build -t tradestation-premium .",
    "docker:run": "docker run -p 3000:3000 --env-file .env tradestation-premium",
    "docker:compose": "docker-compose up -d",
    "security:audit": "npm audit",
    "security:fix": "npm audit fix",
    "prepare": "husky install"
  },
  "keywords": [
    "digital-contract",
    "e-signature",
    "pdf-generation",
    "nodejs",
    "express",
    "mongodb",
    "tradestation",
    "premium"
  ],
  "author": "TradeStation Development Team",
  "license": "MIT",
  "engines": {
    "node": ">=16.0.0",
    "npm": ">=8.0.0"
  },
  "dependencies": {
    "express": "^4.18.2",
    "mongoose": "^7.5.0",
    "bcrypt": "^5.1.1",
    "jsonwebtoken": "^9.0.2",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "multer": "^1.4.5-lts.1",
    "nodemailer": "^6.9.4",
    "node-cron": "^3.0.2",
    "express-rate-limit": "^6.10.0",
    "helmet": "^7.0.0",
    "compression": "^1.7.4",
    "morgan": "^1.10.0",
    "pdfkit": "^0.13.0",
    "qrcode": "^1.5.3",
    "sharp": "^0.32.5",
    "validator": "^13.11.0",
    "winston": "^3.10.0",
    "winston-daily-rotate-file": "^4.7.1"
  },
  "devDependencies": {
    "nodemon": "^3.0.1",
    "jest": "^29.6.4",
    "supertest": "^6.3.3",
    "eslint": "^8.47.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-node": "^11.1.0",
    "prettier": "^3.0.2",
    "husky": "^8.0.3",
    "lint-staged": "^14.0.1",
    "@types/jest": "^29.5.4"
  },
  "lint-staged": {
    "*.js": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{json,md}": [
      "prettier --write"
    ]
  },
  "jest": {
    "testEnvironment": "node",
    "coverageDirectory": "coverage",
    "collectCoverageFrom": [
      "**/*.js",
      "!node_modules/**",
      "!coverage/**",
      "!logs/**"
    ],
    "testMatch": [
      "**/__tests__/**/*.js",
      "**/?(*.)+(spec|test).js"
    ]
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/tradestation/premium-contract-system.git"
  },
  "bugs": {
    "url": "https://github.com/tradestation/premium-contract-system/issues"
  },
  "homepage": "https://github.com/tradestation/premium-contract-system#readme"
}
